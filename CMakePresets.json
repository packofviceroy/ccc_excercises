{
    "version": 8,
    "configurePresets": [
        {
            "name": "air compilers",
            "displayName": "GCC 13.2.0 aarch64-apple-darwin23",
            "description": "Using compilers: C = /opt/homebrew/bin/aarch64-apple-darwin23-gcc-13, CXX = /opt/homebrew/bin/aarch64-apple-darwin23-g++-13",
            "binaryDir": "${sourceDir}/out/build/${presetName}",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/out/install/${presetName}",
                "CMAKE_C_COMPILER": "/opt/homebrew/bin/aarch64-apple-darwin23-gcc-13",
                "CMAKE_CXX_COMPILER": "/opt/homebrew/bin/aarch64-apple-darwin23-g++-13",
                "CMAKE_BUILD_TYPE": "Debug"
            }
        }
    ]
}